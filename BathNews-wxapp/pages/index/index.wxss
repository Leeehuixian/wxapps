/**index.wxss**/
.mainView{
  width:100%;
}
.topTabBar{
  display:flex;
  flex-direction: row;
  width:100%;
  height:40px;
  border-bottom:1px solid #f7f7f7;
}
.leftView{
  width:85%;
  height:100%;
}
.rightView{
  width:15%;
  height:100%;
}
.accountCenter{
  width:48rpx;
  height:48rpx;
  display:block;
  margin:16rpx auto;
}
.tpscview{
  display:flex;
  white-space:nowrap;
  width:100%;
  height:100%;
  line-height: 40px;
  font-size:14px;
  font-weight: bold;
}
.topdatalist{
  display:inline-block;
  margin-left:10px;
  width:50px;
  height:100%;
  text-align: center;
  z-index:99;
}
.curPage{
  border-bottom:2px solid #d75b5d;
  color:#d75b5d;
  height:95%;
}
.subView{
  width:100%;
  box-sizing: border-box;
  padding:10rpx 20rpx;
}
.detailCell{
  border-bottom:1px solid #f7f7f7;
  padding:10rpx 0 20rpx;
}
.cellTitle{
  width:100%;
  font-size:14px;
  font-weight: bold;
  line-height: 2;
}
.imageContainer{
  display:flex;
  flex-flow: row nowrap;
  width:100%;
  height:150rpx;
  margin:10rpx auto;
}
.imageshow{
  width:220rpx;
  height:150rpx;
  margin-right:10rpx;
}
.cellbottom{
  width:100%;
  line-height: 1.5;
  font-size:12px;
  color:#969696;
}
.videoshow{
  width:100%;
}



