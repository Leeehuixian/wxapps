/* page/component/list/list.wxss */
@import '../../common/common.wxss';
.banner image{
  width:100%;
  height:400rpx;
}

.list-box{
  padding:30rpx 20rpx;
  display:flex;
  flex-flow:row wrap;
  justify-content:space-between;
}

.newest-list{
  display:flex;
  flex-flow:column wrap;
  justify-content:center;
  align-items:center;
  width:347rpx;
  height:347rpx;
  margin-bottom:10rpx;
  border-radius:10px;
  background:#f5f6f5;
}

.newest-list image{
  width:180rpx;
  height:180rpx;
}

.newest-list .newest-text{
  font-size:32rpx;
  line-height:45rpx;
}

