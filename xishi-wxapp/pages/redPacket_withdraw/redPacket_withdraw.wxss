page{
  width:100%;
  height:100%;
  background: #f3f5f9;
}

.container{
  width:100%;
  height:100%;
}

.contact-btn{
  position: fixed;
  top:0;
  left:0;
  z-index:10;
  display:initial;
   width:100%;
  height:70rpx;
  margin-left:0;
  margin-right:0;
  padding-left:20rpx;
  padding-right:20rpx;
  box-sizing: border-box;
  border-radius:unset;
  -webkit-tap-highlight-color:transparent;
  overflow:unset;
  line-height: 70rpx;
  text-align:left;
  font-size:28rpx;
  color:#fff;
  background-color:#f32a43;
}

.contact-btn::after{
  content:" ";
  width:0;
  height:0;
  position:unset;
  top:0;
  left:0;
  -webkit-transform:none;
  transform:none;
  -webkit-transform-origin:unset;
  transform-origin:unset;
  box-sizing:unset;
  border-radius:unset;
  border:unset;
} 

.editor_box{
  position: fixed;
  top:130rpx;
  left:30rpx;
  z-index:10;
  width:690rpx;
  background:#fff;
}

.first-line{
  width:100%;
  height:120rpx;
  line-height:120rpx;
  background:#fafafa;
  box-sizing: border-box;
  padding:0 50rpx;
}

.second-section{
  width:690rpx;
  box-sizing: border-box;
  padding:0 50rpx;
}

.input_wrap{
  display:flex;
  align-items: center;
  width:100%;
  height:140rpx;
  font-size:48rpx;
  color:#000;
  border-bottom:1px solid #c8c8c8;
}

.input_wrap input{
  height:100%;
  font-size:48rpx;
  color:#000;
}

.withdraw_btn{
  width:590rpx;
  height:80rpx;
  text-align: center;
  line-height: 80rpx;
  font-size:32rpx;
  color:#fff;
  border-radius: 5px;
  background: #1aac19;
  opacity: .5;
}

.withdraw-active{
  opacity: 1;
}

.tip_txt{
  width:100%;
  height:100rpx;
  line-height:100rpx;
  text-align:center;
  font-size:24rpx;
  color:#969696;
}





