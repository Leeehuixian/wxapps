.contact-btn{
  position: fixed;
  top:0;
  left:0;
  z-index:10;
  display:initial;
   width:100%;
  height:70rpx;
  margin-left:0;
  margin-right:0;
  padding-left:20rpx;
  padding-right:20rpx;
  box-sizing: border-box;
  border-radius:unset;
  -webkit-tap-highlight-color:transparent;
  overflow:unset;
  line-height: 70rpx;
  text-align:left;
  font-size:28rpx;
  color:#fff;
  background-color:#f32a43;
}

.contact-btn::after{
  content:" ";
  width:0;
  height:0;
  position:unset;
  top:0;
  left:0;
  -webkit-transform:none;
  transform:none;
  -webkit-transform-origin:unset;
  transform-origin:unset;
  box-sizing:unset;
  border-radius:unset;
  border:unset;
} 

/*红包信息*/
.sender-info{
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width:100%;
  height:480rpx;
  margin-top:70rpx;
  box-sizing:border-box;
  padding-top:30rpx;
  background:#f32a43;
}

.sender-info .avatar{
  width:182rpx;
  height:182rpx;
  margin:0 0 10rpx;
  border-radius: 50%;
  border:2px solid #fff;
}

.avatar image{
  width:182rpx;
  height:182rpx;
  border-radius: 50%;
}

.sender-info .nickname{
  margin-bottom:20rpx;
  font-size:32rpx;
  color:#fff;
}

.show{
  display:block;
}

.hidden{
  display:none;
}

/*语音框*/
.voice_wrap{
  position: relative;
  width:400rpx;
  height:70rpx;
  margin-bottom:40rpx;
  border-radius: 20px;
  text-align: center;
  line-height: 70rpx;
  font-size:28rpx;
  color:#fff;
  background:#1aac19; 
}

.voice_icon{
  position: absolute;
  left:20rpx;
  top:16rpx;
  z-index:10;
  width:27rpx;
  height:38rpx;
}

.redPacket-info{
  width:100%;
  height:80rpx;
  box-sizing: border-box;
  padding-left:20rpx;
  line-height: 80rpx;
  font-size:30rpx;
  color:#969696;
}

/*领取列表*/
.receive-list{
  width:100%;
  padding-bottom:110rpx;
}

.receive-item{
  display:flex;
  align-items: center;
  width:100%;
  height:140rpx;
  box-sizing: border-box;
  padding:0 20rpx;
  border-top:1px solid #ebebeb;
}

.recipient-avatar{
  width:80rpx;
  height:80rpx;
  margin-right:20rpx;
  border-radius: 10px;
  flex:none;
}

.recipient-info{
  display:flex;
  flex-flow: column wrap;
  justify-content: center;
  width:610rpx;
  height:100%;
}

.recipient-info view{
  display:flex;
  justify-content: space-between;
  line-height: 1.5;
}

.recipient-info .first-line{
  font-size:30rpx;
  color:#000;
}

.recipient-info .second-line{
  color:#969696;
  font-size:26rpx;
}

.no-more{
  width:100%;
  height:100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size:28rpx;
  color:#969696;
  border-top:1px solid #ebebeb;
}

/*功能栏*/
.function-entrys{
  position: fixed;
  bottom:0;
  left:0;
  z-index:10;
  display:flex;
  justify-content: space-around;
  align-items: center;
  width:100%;
  height:110rpx;
  background: #f5f5f5;
  box-sizing: border-box;
  padding:0 20rpx;
}

.function-entrys view{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  text-align: center;
  color:#969696;
  font-size:24rpx;
}

.function-entrys view image{
  width:42rpx;
  height:44rpx;
}

.share-btn{
  position:initial;
  display:initial;
  /* width:44rpx;
  height:46rpx; */
  width:100%;
  height:100%;
  margin-left:0;
  margin-right:0;
  padding-left:0;
  padding-right:0;
  padding-top:42rpx;
  box-sizing:border-box;
  border-radius:unset;
  -webkit-tap-highlight-color:transparent;
  overflow:unset;
  color:#969696;
  font-size:24rpx;
  background-color:unset;
  background:url(data:image/png;base64,) no-repeat center 15rpx; 
  background-size:38rpx 42rpx;
}

.share-btn::after{
  content:" ";
  width:0;
  height:0;
  position:unset;
  top:0;
  left:0;
  -webkit-transform:none;
  transform:none;
  -webkit-transform-origin:unset;
  transform-origin:unset;
  box-sizing:unset;
  border-radius:unset;
  border:unset;
}

/*遮罩层*/
.modal-bg{
  position:fixed;
  left:0;
  top:0;
  z-index:200;
  width:100%;
  height:100%;
  background:rgba(0,0,0,.5);
}

.canvas-view{
  position:fixed;
  left:50%;
  top:50rpx;
  z-index:201;
  width:265px;
  height:411px;
  margin-left:-132.5px;
  border-radius:10px;
  background:#fff;
}

.my-canvas{
  width:239px;
  height:385px;
  background:#fff;
  border:none;
  margin:13px auto;
}

.save-image{
  position: fixed;
  bottom:50rpx;
  left:50%;
  z-index:201;
  width:380rpx;
  height:80rpx;
  margin-left:-190rpx;
  text-align: center;
