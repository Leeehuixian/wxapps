page{
  width:100%;
  height:100%;
  background: #f3f5f9;
}

.container{
  width:100%;
  height:100%;
}

.editor_box{
  position: fixed;
  top:60rpx;
  left:30rpx;
  z-index:10;
  width:690rpx;
  background:#fff;
}

.first-line{
  width:100%;
  height:120rpx;
  line-height:120rpx;
  background:#fafafa;
  box-sizing: border-box;
  padding:0 50rpx;
}

.second-section{
  width:690rpx;
  box-sizing: border-box;
  padding:0 50rpx;
}

.input_wrap{
  display:flex;
  align-items: center;
  width:100%;
  height:140rpx;
  font-size:48rpx;
  color:#000;
  border-bottom:1px solid #c8c8c8;
}

.input_wrap input{
  height:100%;
  font-size:48rpx;
  color:#000;
}

.withdraw_btn{
  width:590rpx;
  height:80rpx;
  text-align: center;
  line-height: 80rpx;
  font-size:32rpx;
  color:#fff;
  border-radius: 5px;
  background: #1aac19;
  opacity: .5;
}

.withdraw-active{
  opacity: 1;
}

.tip_txt{
  width:100%;
  height:100rpx;
  line-height:100rpx;
  text-align:center;
  font-size:24rpx;
  color:#969696;
}





