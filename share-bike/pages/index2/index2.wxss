/* pages/index2/index2.wxss */
.mb-map{
  width:100%;
  height:100%;
}
.mb-new{
  width:690rpx;
  height:100rpx;
  background-color:#fff;
  position:fixed;
  top:30rpx;
  left:30rpx;
  z-index: 1000;
  box-shadow: 0 2px 5px #ccc;
  line-height: 100rpx;
}
.mb-new image{
  float:left;
  display:block;
  width:50rpx;
  height:50rpx;
  margin: 25rpx 20rpx 0 30rpx;
}
.mb-new .right{
  float:right;
  margin-right: 30rpx;
  color: #ccc;
}