/* pages/warn/warn.wxss */
.choose{
  width:100%;
  background:#fff;
}
.block_tit{
  width:100%;
  height:80rpx;
  background:#eee;
  color:#000;
  line-height: 80rpx;
  box-sizing: border-box;
  padding-left:40rpx;
}
.choose-grids{
  display:flex;
  flex-wrap:wrap;
  justify-content: space-around;
  padding:50rpx 50rpx 14rpx;
}
.choose-grids .grid{
  width:45%;
  height:100rpx;
  margin-bottom:36rpx;
  border-radius: 6rpx;
  line-height: 100rpx;
  text-align: center;
  border:2rpx solid limegreen;
}

